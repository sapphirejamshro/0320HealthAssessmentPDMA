<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:andriod="http://schemas.android.com/apk/res-auto"
    android:keyWidth="9.8%p"
    android:horizontalGap="5px"
    android:verticalGap="3dp"
    android:keyHeight="50dp">
    <Row android:keyWidth="11%p" android:rowEdgeFlags="top" android:horizontalGap="0dp">
        <Key android:codes="500" android:horizontalGap="5px" android:keyIcon="@drawable/ic_smiley" android:keyWidth="10%p"  android:keyEdgeFlags="left"/>
        <Key android:keyLabel=" " android:keyWidth="77.3%p"/>
        <Key android:codes="-101" android:keyIcon="@drawable/globe" android:keyWidth="10%p" android:keyEdgeFlags="right"/>
    </Row>
    <Row android:keyWidth="11%p">
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="9.1%p"
            android:popupCharacters="+" android:keyLabel="+" android:keyEdgeFlags="left"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="9.1%p"
            android:popupCharacters="×" android:keyLabel="×"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="9.1%p"
            android:popupCharacters="÷" android:keyLabel="÷" />
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="9.1%p"
            android:popupCharacters="=" android:keyLabel="="/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="9.1%p"
            android:popupCharacters="/" android:keyLabel= "/"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="9.1%p"
            android:popupCharacters="_" android:keyLabel= "_"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="9%p"
            android:popupCharacters="€" android:keyLabel="€"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="9%p"
            android:popupCharacters="£" android:keyLabel="£"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="9%p"
            android:popupCharacters="¥" android:keyLabel="¥" />
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="9%p"
            android:popupCharacters="₩" android:keyLabel="₩"
            android:keyEdgeFlags="right"/>

    </Row>
    <Row android:keyWidth="11%p">
        <Key android:keyLabel="!" android:keyEdgeFlags="left" android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="8.2%p"
            android:popupCharacters="!" android:horizontalGap="5.5%p"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="8.2%p"
            android:popupCharacters="\@" android:keyLabel="\@"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="8.2%p"
            android:popupCharacters="#" android:keyLabel="#"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="8.2%p"
            android:popupCharacters="$" android:keyLabel="$"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="8.2%p"
            android:popupCharacters="%" android:keyLabel="%" />
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="8.2%p"
            android:popupCharacters="^" android:keyLabel="^" />
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="8.2%p"
            android:popupCharacters="&amp;" android:keyLabel="&amp;"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="8.2%p"
            android:popupCharacters="*" android:keyLabel="*"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="8.2%p"
            android:popupCharacters="(" android:keyLabel="(" />
        <Key android:keyLabel=")" android:keyEdgeFlags="right" android:keyWidth="8.2%p"/>

    </Row>

    <Row android:keyWidth="11%p">
        <Key android:codes="510" android:keyLabel="1/2"
            android:keyWidth="14%p" android:keyEdgeFlags="left"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="9.1%p"
            android:popupCharacters="-" android:keyLabel="-"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="9.1%p"
            android:popupCharacters="'" android:keyLabel="'"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="9.1%p"
            android:popupCharacters="&quot;" android:keyLabel="&quot;"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="9.1%p"
            android:popupCharacters=":" android:keyLabel=":" />
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="9.1%p"
            android:popupCharacters=";" android:keyLabel= ";"/>
        <Key android:popupKeyboard="@xml/keyboard_popup_template" android:keyWidth="9%p"
            android:popupCharacters="," android:keyLabel="," />
        <Key android:keyLabel="\?" android:popupCharacters="\?" android:keyWidth="9%p"/>
        <!--<Key android:keyLabel="❝" android:popupCharacters="❝" android:keyWidth="9%p"/>-->
        <Key android:codes="-5" android:keyIcon="@drawable/ic_back_en" android:keyWidth="13.8%p"
            android:isRepeatable="true" android:keyEdgeFlags="right"/>

    </Row>

    <Row android:rowEdgeFlags="bottom" android:horizontalGap="5px">
        <Key android:codes="505" android:keyLabel="ABC" android:keyWidth="14%p"  android:keyEdgeFlags="left"/>
        <Key android:keyLabel="," android:keyWidth="9.1%p" />
        <Key android:codes="32" android:keyLabel=" " android:keyWidth="48.6%p" />
        <Key android:keyLabel="." android:keyWidth="9.1%p" />
        <Key android:codes="-4" android:keyIcon="@drawable/ic_enter" android:keyWidth="13.8%p" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>