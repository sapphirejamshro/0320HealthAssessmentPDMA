<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:elevation="0dp"
    android:paddingBottom="5dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <ImageView
        android:id="@+id/imgViewSeekMedicalHelpAdapter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="3dp"
        android:src="@mipmap/difficulty_breathing"
        android:layout_gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />
            <TextView
                android:id="@+id/tvNameSeekMedicalHelpAdapter"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Text"
                android:textAllCaps="false"
                android:textColor="@color/appColorBlack"
                android:textAlignment="center"
                android:gravity="center_vertical"
                android:textSize="20sp"
                android:fontFamily="@font/myriad_pro_regular"
                android:paddingTop="5dp"
                android:textStyle="bold"
                android:paddingBottom="0dp"
                android:paddingRight="8dp"
                android:paddingLeft="8dp"
                android:paddingStart="8dp"
                android:paddingEnd="8dp"
                app:layout_constraintTop_toBottomOf="@+id/imgViewSeekMedicalHelpAdapter"
                app:layout_constraintRight_toRightOf="@+id/imgViewSeekMedicalHelpAdapter"
                app:layout_constraintEnd_toEndOf="@+id/imgViewSeekMedicalHelpAdapter"
                app:layout_constraintStart_toStartOf="@+id/imgViewSeekMedicalHelpAdapter"
                app:layout_constraintLeft_toLeftOf="@+id/imgViewSeekMedicalHelpAdapter"

                />
        </androidx.constraintlayout.widget.ConstraintLayout>
