<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.UserInformationFragment"
    android:focusableInTouchMode="true"
    android:focusable="true"
    android:clickable="true"
    >

    <ScrollView
        android:id="@+id/scrollView_UIFrag"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="12dp"
        android:layout_marginLeft="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginStart="10dp"
        android:overScrollMode="never"
        android:scrollbarAlwaysDrawVerticalTrack="true"
        android:scrollbarSize="12dp"
        android:scrollbarStyle="insideOverlay"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:orientation="vertical"
        android:isScrollContainer="false"
        android:scrollbars="none">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusableInTouchMode="true"
            android:focusable="true"
            android:clickable="true"
            android:layout_marginTop="10dp"
            >

            <LinearLayout
                android:id="@+id/ll_info_tilte_container_user_info_frag"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:weightSum="1"
                android:layout_marginTop="0dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                >


                <View
                    android:id="@+id/view_line_info_left_user_info_frag"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="0.5"
                    android:background="@color/grey"
                    android:layout_marginRight="5dp"
                    android:layout_marginEnd="5dp"
                    />

                <TextView
                    android:id="@+id/tv_info_user_info_frag"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="INFORMATION"
                    android:textStyle="bold"
                    android:textSize="15sp"
                    android:textColor="@color/appColorGreen"
                    android:fontFamily="@font/myriad_pro_bold"
                    />

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="0.5"
                    android:layout_marginStart="5dp"
                    android:layout_marginLeft="5dp"
                    android:background="@color/grey"/>
            </LinearLayout>



            <TextView
                android:id="@+id/tv_name_user_info_frag"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/name_text"
                android:textColor="@color/appColorBlack"
                android:textSize="18sp"
                android:includeFontPadding="false"
                android:layout_marginTop="0dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                app:layout_constraintTop_toBottomOf="@+id/ll_info_tilte_container_user_info_frag"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                android:fontFamily="@font/myriad_pro_regular"
                />
            <TextView
                android:id="@+id/tvNameErrorUIFrag"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:paddingTop="2dp"
                android:paddingBottom="0dp"
                android:fontFamily="@font/myriad_pro_regular"
                android:text="Name is required"
                android:textColor="@color/requireRedColor"
                android:textSize="14sp"
                android:visibility="gone"
                app:layout_constraintBottom_toTopOf="@id/cv_name_container_user_info_frag"
                app:layout_constraintLeft_toLeftOf="@id/tv_name_user_info_frag"
                app:layout_constraintStart_toStartOf="@id/tv_name_user_info_frag"
                app:layout_constraintTop_toBottomOf="@id/tv_name_user_info_frag" />

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_name_container_user_info_frag"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="2dp"
                app:cardElevation="2dp"
                app:cardMaxElevation="2dp"
                android:layout_marginTop="4dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintTop_toBottomOf="@+id/tvNameErrorUIFrag"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                >

                <com.sapphire.HealthAssessmentPDMA.helper.BackPressAwareEdittext
                    android:id="@+id/edNameUIFrag"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Muhammad Ahmed"
                    android:background="@null"
                    android:textSize="20sp"
                    android:textColor="@color/appColorBlack"
                    android:maxLines="1"
                    android:inputType="text"
                    android:paddingTop="14dp"
                    android:paddingBottom="14dp"
                    android:paddingStart="10dp"
                    android:paddingLeft="10dp"
                    android:paddingEnd="10dp"
                    android:paddingRight="10dp"
                    android:fontFamily="@font/myriad_pro_regular"
                    />

            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/tv_mbl_no_user_info_frag"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/mobile_no_text"
                android:textColor="@color/appColorBlack"
                android:textSize="18sp"
                android:includeFontPadding="false"
                android:layout_marginTop="3dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                app:layout_constraintTop_toBottomOf="@+id/cv_name_container_user_info_frag"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"

                />

            <TextView
                android:id="@+id/tvMobileNoErrorUIFrag"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:paddingTop="2dp"
                android:paddingBottom="0dp"
                android:fontFamily="@font/myriad_pro_regular"
                android:text="Mobile # is required"
                android:textColor="@color/requireRedColor"
                android:textSize="14sp"
                android:visibility="gone"
                app:layout_constraintBottom_toTopOf="@id/cv_mbl_no_container_user_info_frag"
                app:layout_constraintLeft_toLeftOf="@id/tv_mbl_no_user_info_frag"
                app:layout_constraintStart_toStartOf="@id/tv_mbl_no_user_info_frag"
                app:layout_constraintTop_toBottomOf="@id/tv_mbl_no_user_info_frag" />

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_mbl_no_container_user_info_frag"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="2dp"
                app:cardElevation="2dp"
                app:cardMaxElevation="2dp"
                android:layout_marginTop="4dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintTop_toBottomOf="@+id/tvMobileNoErrorUIFrag"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                >

                <EditText
                    android:id="@+id/edMobileNumber"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="+923331234567"
                    android:background="@null"
                    android:textSize="20sp"
                    android:textColor="@color/appColorBlack"
                    android:maxLines="1"
                    android:inputType="number"
                    android:maxLength="13"
                    android:paddingTop="14dp"
                    android:paddingBottom="14dp"
                    android:paddingStart="10dp"
                    android:paddingLeft="10dp"
                    android:fontFamily="@font/myriad_pro_regular"
                    />

            </androidx.cardview.widget.CardView>

            <LinearLayout
                android:id="@+id/llGenderAge"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@color/transparent"
                android:weightSum="2"
                android:gravity="bottom"
                app:layout_constraintTop_toBottomOf="@id/cv_mbl_no_container_user_info_frag"
                app:layout_constraintLeft_toLeftOf="@id/cv_mbl_no_container_user_info_frag"
                app:layout_constraintStart_toStartOf="@id/cv_mbl_no_container_user_info_frag"
                app:layout_constraintEnd_toEndOf="@id/cv_mbl_no_container_user_info_frag"
                app:layout_constraintRight_toRightOf="@id/cv_mbl_no_container_user_info_frag"
                >

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"

                    >
                    <TextView
                        android:id="@+id/tvAgeUIFrag"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/age"
                        android:textColor="@color/appColorBlack"
                        android:textSize="18sp"
                        android:includeFontPadding="false"
                        android:layout_marginTop="5dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:fontFamily="@font/myriad_pro_regular"
                        />
                    <TextView
                        android:id="@+id/tvAgeErrorUIFrag"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="0dp"
                        android:paddingTop="2dp"
                        android:paddingBottom="0dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:fontFamily="@font/myriad_pro_regular"
                        android:text="Age is required"
                        android:textColor="@color/requireRedColor"
                        android:textSize="14sp"
                        android:visibility="gone"
                        />
                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginTop="4dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="4dp"
                        android:layout_marginEnd="4dp"
                        android:layout_marginBottom="5dp"
                        android:elevation="2dp"
                        app:cardElevation="2dp"
                        app:cardCornerRadius="2dp"
                        app:cardBackgroundColor="@color/transparent">
                        <EditText
                            android:id="@+id/edAgeUIFrag"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="34"
                            android:background="@color/appColorWhite"
                            android:textSize="20sp"
                            android:textColor="@color/appColorBlack"
                            android:maxLines="1"
                            android:inputType="number"
                            android:maxLength="3"
                            android:paddingTop="11.3dp"
                            android:paddingBottom="11.3dp"
                            android:paddingStart="10dp"
                            android:paddingLeft="10dp"
                            android:fontFamily="@font/myriad_pro_regular"
                            />

                    </androidx.cardview.widget.CardView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="center_vertical"
                    >
                    <TextView
                        android:id="@+id/tvGenderUIFrag"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/gender"
                        android:textColor="@color/appColorBlack"
                        android:textSize="18sp"
                        android:includeFontPadding="false"
                        android:layout_marginTop="5dp"
                        android:layout_marginStart="4dp"
                        android:layout_marginLeft="4dp"
                        android:fontFamily="@font/myriad_pro_regular"

                        />

                    <TextView
                        android:id="@+id/tvGenderErrorUIFrag"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="0dp"
                        android:paddingTop="2dp"
                        android:paddingBottom="0dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:fontFamily="@font/myriad_pro_regular"
                        android:text="Gender is required"
                        android:textColor="@color/requireRedColor"
                        android:textSize="14sp"
                        android:visibility="gone"
                        />
                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginTop="4dp"
                        android:layout_marginStart="4dp"
                        android:layout_marginLeft="4dp"
                        android:layout_marginRight="8dp"
                        android:layout_marginEnd="8dp"
                        android:layout_marginBottom="5dp"

                        android:elevation="2dp"
                        app:cardElevation="2dp"
                        app:cardCornerRadius="2dp"
                        app:cardBackgroundColor="@color/transparent">
                        <Spinner
                            android:id="@+id/spGender"
                            android:layout_width="match_parent"
                            android:paddingTop="3dp"
                            android:paddingBottom="3dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/spinner_bg"
                            tools:listitem="@layout/custom_spinner_item"
                            />

                    </androidx.cardview.widget.CardView>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/llCityDist"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@color/transparent"
                android:gravity="bottom"
                app:layout_constraintTop_toBottomOf="@id/llGenderAge"
                app:layout_constraintLeft_toLeftOf="@id/llGenderAge"
                app:layout_constraintStart_toStartOf="@id/llGenderAge"
                app:layout_constraintEnd_toEndOf="@id/llGenderAge"
                app:layout_constraintRight_toRightOf="@id/llGenderAge"
                >

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    >
                    <TextView
                        android:id="@+id/tv_dist_user_info_frag"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/dist_text"
                        android:textColor="@color/appColorBlack"
                        android:textSize="18sp"
                        android:includeFontPadding="false"
                        android:layout_marginTop="0dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:fontFamily="@font/myriad_pro_regular"
                        />
                    <TextView
                        android:id="@+id/tvDistrictErrorUIFrag"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="0dp"
                        android:paddingTop="2dp"
                        android:paddingBottom="0dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:fontFamily="@font/myriad_pro_regular"
                        android:text="District is required"
                        android:textColor="@color/requireRedColor"
                        android:textSize="14sp"
                        android:visibility="gone"
                        />
                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginTop="4dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="4dp"
                        android:layout_marginEnd="4dp"
                        android:layout_marginBottom="5dp"
                        android:elevation="2dp"
                        app:cardElevation="2dp"
                        app:cardCornerRadius="2dp"
                        app:cardBackgroundColor="@color/transparent">
                        <Spinner
                            android:id="@+id/spin_dist_user_info_frag"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingTop="4dp"
                            android:paddingBottom="4dp"
                            android:background="@drawable/spinner_bg"
                            tools:listitem="@layout/custom_spinner_item"
                            />
                    </androidx.cardview.widget.CardView>
                </LinearLayout>


                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    >
                    <TextView
                        android:id="@+id/tv_tehsil_user_info_frag"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tehsil_text"
                        android:textColor="@color/appColorBlack"
                        android:textSize="18sp"
                        android:includeFontPadding="false"
                        android:layout_marginTop="0dp"
                        android:layout_marginStart="4dp"
                        android:layout_marginLeft="4dp"
                        android:fontFamily="@font/myriad_pro_regular"
                        />
                    <TextView
                        android:id="@+id/tvTehsilErrorUIFrag"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="0dp"
                        android:paddingTop="2dp"
                        android:paddingBottom="0dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginLeft="8dp"
                        android:fontFamily="@font/myriad_pro_regular"
                        android:text="Tehsil is required"
                        android:textColor="@color/requireRedColor"
                        android:textSize="14sp"
                        android:visibility="gone"
                        />
                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginTop="4dp"
                        android:layout_marginStart="4dp"
                        android:layout_marginLeft="4dp"
                        android:layout_marginRight="8dp"
                        android:layout_marginEnd="8dp"
                        android:layout_marginBottom="5dp"
                        android:elevation="2dp"
                        app:cardElevation="2dp"
                        app:cardCornerRadius="2dp"
                        app:cardBackgroundColor="@color/transparent">
                        <Spinner
                            android:id="@+id/spin_tehsil_user_info_frag"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingTop="4dp"
                            android:paddingBottom="4dp"
                            android:background="@drawable/spinner_bg"
                            tools:listitem="@layout/custom_spinner_item"
                            />
                    </androidx.cardview.widget.CardView>
                </LinearLayout>




            </LinearLayout>
            <TextView
                android:id="@+id/tv_address_user_info_frag"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/address_text"
                android:textColor="@color/appColorBlack"
                android:textSize="18sp"
                android:includeFontPadding="false"
                android:layout_marginTop="5dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                app:layout_constraintTop_toBottomOf="@+id/llCityDist"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                android:fontFamily="@font/myriad_pro_regular"
                />

            <TextView
                android:id="@+id/tvAddressErrorUIFrag"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:paddingTop="2dp"
                android:paddingBottom="0dp"
                android:fontFamily="@font/myriad_pro_regular"
                android:text="Address is required"
                android:textColor="@color/requireRedColor"
                android:textSize="14sp"
                android:visibility="gone"
                app:layout_constraintTop_toBottomOf="@id/tv_address_user_info_frag"
                app:layout_constraintStart_toStartOf="@id/tv_address_user_info_frag"
                app:layout_constraintLeft_toLeftOf="@id/tv_address_user_info_frag"
                />

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_address_container_user_info_frag"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="2dp"
                app:cardElevation="2dp"
                app:cardMaxElevation="2dp"
                android:layout_marginTop="4dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintTop_toBottomOf="@+id/tvAddressErrorUIFrag"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                >

                <com.sapphire.HealthAssessmentPDMA.helper.BackPressAwareEdittext
                    android:id="@+id/edAddressUIFrag"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="House#1548, Block 2, Ghulshane Iqbal Karachi"
                    android:background="@null"
                    android:textSize="20sp"
                    android:textColor="@color/appColorBlack"
                    android:minLines="3"
                    android:maxLines="3"
                    android:gravity="top"
                    android:imeOptions="actionNext"
                    android:inputType="textMultiLine"
                    android:paddingTop="16dp"
                    android:paddingBottom="16dp"
                    android:paddingStart="10dp"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:paddingEnd="10dp"
                    android:fontFamily="@font/myriad_pro_regular"
                    android:textDirection="inherit"
                    />

            </androidx.cardview.widget.CardView>




            <LinearLayout
                android:id="@+id/ll_add_info_tilte_container_user_info_frag"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:weightSum="1"
                android:layout_marginTop="40dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginEnd="8dp"
                app:layout_constraintTop_toBottomOf="@+id/cv_address_container_user_info_frag"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                >


                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="0.5"
                    android:background="@color/grey"
                    android:layout_marginRight="5dp"
                    android:layout_marginEnd="5dp"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="OPTIONAL INFORMATION"
                    android:textStyle="bold"
                    android:textSize="15sp"
                    android:textColor="@color/appColorGreen"
                    android:fontFamily="@font/myriad_pro_bold"
                    />

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="0.5"
                    android:layout_marginStart="5dp"
                    android:layout_marginLeft="5dp"
                    android:background="@color/grey"/>
            </LinearLayout>


            <TextView
                android:id="@+id/tv_cnic_user_info_frag"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/cnic_text"
                android:textColor="@color/appColorBlack"
                android:textSize="18sp"
                android:includeFontPadding="false"
                android:layout_marginTop="5dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                app:layout_constraintTop_toBottomOf="@+id/ll_add_info_tilte_container_user_info_frag"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                android:fontFamily="@font/myriad_pro_regular"
                />
            <TextView
                android:id="@+id/tvCnicErrorUIFrag"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:paddingTop="2dp"
                android:paddingBottom="0dp"
                android:fontFamily="@font/myriad_pro_regular"
                android:text="CNIC is required"
                android:textColor="@color/requireRedColor"
                android:textSize="14sp"
                android:visibility="gone"
